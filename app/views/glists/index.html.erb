<p id="notice"><%= notice %></p>

<h1>Glists</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @glists.each do |glist| %>
      <tr>
        <td><%= link_to 'Show', glist %></td>
        <td><%= link_to 'Edit', edit_glist_path(glist) %></td>
        <td><%= link_to 'Destroy', glist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Glist', new_glist_path %>
