<p id="notice"><%= notice %></p>

<h1>Glist Items</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @glist_items.each do |glist_item| %>
      <tr>
        <td><%= link_to 'Show', glist_item %></td>
        <td><%= link_to 'Edit', edit_glist_item_path(glist_item) %></td>
        <td><%= link_to 'Destroy', glist_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Glist Item', new_glist_item_path %>
