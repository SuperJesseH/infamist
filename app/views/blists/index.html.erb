<p id="notice"><%= notice %></p>

<h1>Blists</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @blists.each do |blist| %>
      <tr>
        <td><%= link_to 'Show', blist %></td>
        <td><%= link_to 'Edit', edit_blist_path(blist) %></td>
        <td><%= link_to 'Destroy', blist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Blist', new_blist_path %>
